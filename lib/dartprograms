----------Map---------------
void main(){
  var gifts ={
    
    'first' : "ring",
    'second' : "chain",
    'fifth'  :"bangles"
    
  };
  
  var nobleGases ={
    2 :'helium',
    10 : 'neon',
    18 :'argon'
    
  };
  
  var gifts = Map<String ,String>();
  gifts['first'] ="ring";
  gifts["second"] ="chain";
  gifts["fifth"] ="bangles";
  
  

var nobleGases = Map<int, String>();
nobleGases[2] = 'helium';
nobleGases[10] = 'neon';
nobleGases[18] = 'argon';
  

}
