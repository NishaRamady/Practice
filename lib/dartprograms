void main(){
  int num1 =10;
  int num2 = 5;
  int sum = num1 + num2;
  int diff = num1 - num2;
  int mult = num1 * num2;
  double div = num1 / num2;
  print("The sum is $sum");
  print("The diff is $diff");
  print("The pro is $mult");
  print("The quo is $div");
  }
