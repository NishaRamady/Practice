void main() {
   List<int> array=[1,2,3,4,5,12,18,34,];
  printEven(num:array);
}

  
   void printEven({required List<int> num}){
     for(int i=0;i<num.length;i++){
       if(num[i]%2 == 0){
         print("even numbers ${num[i]}");
       }
     }
  
}
--------------------------------------- 

void main() {
   List<int> array=[1,2,3,4,5,12,18,34,];
  printEven(array);
}

  
   void printEven( List<int>  array){
     for(int i=0;i<array.length;i++){
       if(array[i]%2 == 0){
         print("even numbers ${array[i]}");
       }
     }
  
}
