//   void printEven({required int num,int? num2,required String data}){
   void printEven({required int num}){
  if (num%2 == 0){
    print("It is  an even number");
  }
  else if(num>5){
    print("number greater than 5,number is $num");
  } 
  
  else{
    print("Number is odd and number is less than $num");
  }
}
