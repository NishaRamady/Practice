  --------Set--------------
  void main(){
  var halogens = {'fluorine','iodine','bromine','astatine','chlorine'};
  var names =<String>{};
//   Set<String> names = {};
  var elements =<String>{};
  elements.add("fluorine");
  elements.addAll(halogens);
  assert(elements.length == 5);
  final constantSet = const{"Fluorine","Chlorine","Bromine","Iodine","Astatine"};
}
